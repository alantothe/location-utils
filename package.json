{
  "name": "url-util",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "dev:python": "turbo dev --filter=@url-util/python-alt-text",
    "dev:all": "bun run scripts/dev-all.js",
    "install:python-deps": "turbo install-deps --filter=@url-util/python-alt-text",
    "test:python": "turbo test:python --filter=@url-util/python-alt-text",
    "build": "turbo build",
    "clean": "turbo clean"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "turbo": "^2.7.1",
    "typescript": "^5.0.0"
  }
}
